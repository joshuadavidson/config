{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:white",
          "background": "p:teal",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "style": "agnoster_full",
            "max_depth": 4
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:white",
          "background": "p:teal",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#139cdd{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff9800{{ end }}",
            "{{ if gt .Ahead 0 }}#fa5a5a{{ end }}",
            "{{ if gt .Behind 0 }}#ff9800{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \udb80\udd93 {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "branch_icon": "\ue725 "
          }
        }
      ]
    },
    {
      "type": "newline",
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:white",
          "template": "\u276f "
        }
      ]
    }
  ],
  "tooltips": [],
  "transient_prompt": {
    "foreground": "p:black",
    "background": "transparent",
    "template": "<p:yellow,transparent>\ue0b6</><,p:yellow> {{ .Folder }} </><p:yellow,transparent>\ue0b0</> "
  },
  "secondary_prompt": {
    "foreground": "p:black",
    "background": "transparent",
    "template": "<p:yellow,transparent>\ue0b6</><,p:yellow> > </><p:yellow,transparent>\ue0b0</> "
  },
  "palette": {
    "black": "#0C0C0C",
    "blue": "#139cdd",
    "green": "#59C9A5",
    "orange": "#ff9800",
    "red": "#fa5a5a",
    "white": "#F2F2F2",
    "yellow": "#F3AE35",
    "teal": "#0f445b"
  }
}